import request from '@/utils/request'

export function getList(options) {
    return request({
        url: '/management-web/http/do.jhtml?router=positionRelationService.list',
        method: 'post',
        data: options
    })
}

export function queryBlockByUnit(options) {
    return request({
        url: '/management-web/http/do.jhtml?router=positionRelationService.queryBlockByUnit',
        method: 'post',
        data: options
    })
}

